<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newface.mybatis.Qna11Mapper">
	<insert id="insert_cate" parameterType="category">
		insert into category values(0,#{name})
	</insert>
	<insert id="insert_item" parameterType="item">
		insert into category values(0,#{pay},#{name},#{content},now(),#{org_name},#{save_name},#{category_num})
	</insert>
	<select id="list" resultType="qna11">
		select qna11_num,title,content,date_format(regdate,'%Y-%m-%d') regdate,confirm,id from qna11
	</select>
	<select id="getinfo" resultType="qna11" parameterType="string">
		select qna11_num,title,content,date_format(regdate,'%Y-%m-%d') regdate,confirm,id from qna11 where id=#{id}
	</select>
</mapper>  